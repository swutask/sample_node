{
  "name": "complex-be",
  "version": "1.0.0",
  "description": "**Edit a file, create a new file, and clone from Bitbucket in under 2 minutes**",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development TZ=UTC node --inspect=127.0.0.1:9229 index.js",
    "start:e2e": "NODE_ENV=e2e TZ=UTC node --inspect=127.0.0.1:9229 index.js",
    "start-pm2": "NODE_ENV=development TZ=UTC node index.js",
    "startd": "NODE_ENV=development TZ=UTC nodemon --inspect=127.0.0.1:9229 index.js",
    "dev": "NODE_ENV=development TZ=UTC nodemon --inspect=127.0.0.1:9229 index.js",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "migrate": "NODE_ENV=development node src/migrate.js",
    "migrate:revert": "NODE_ENV=development node src/migrate.js down",
    "stage": "NODE_ENV=stage TZ=UTC node --inspect=127.0.0.1:9229 index.js",
    "production": "NODE_ENV=production TZ=UTC node index.js",
    "email-notification-consumer:run": "NODE_ENV=development TZ=UTC node src/jobs/email-notification-consumer.js",
    "production-migrate": "NODE_ENV=production node src/migrate.js",
    "docker:env": "docker compose -p complex-env -f docker-compose.env.yml --env-file .env.development up",
    "e2e:docker": "docker compose -p complex-e2e -f docker-compose.e2e.yml --env-file .env.e2e up --build",
    "e2e:docker:rebuild": "docker-compose -f docker-compose.e2e.yml -p complex-e2e --env-file .env.e2e up --build test",
    "e2e:local": "./node_modules/.bin/jest --config test/e2e/jest-e2e.json --verbose --forceExit --detectOpenHandles --runInBand",
    "seed:roles": "NODE_ENV=development node src/scripts/add-roles.js",
    "seed:team-roles": "NODE_ENV=development node src/scripts/add-team-roles.js",
    "seed:update-plans": "NODE_ENV=development node src/scripts/update-plans.js",
    "seed:plans": "NODE_ENV=development node src/scripts/add-plans.js",
    "seed:reactions": "NODE_ENV=development node src/scripts/add-reactions.js",
    "seed:all": "npm run seed:roles && npm run seed:team-roles && npm run seed:plans && npm run seed:reactions",
    "elastic:copy": "NODE_ENV=development node src/scripts/copy-to-elastic.js",
    "elastic:clear": "NODE_ENV=development node src/scripts/clear-elastic.js",
    "activity-consumer:run": "NODE_ENV=development TZ=UTC node src/jobs/activity-consumer.js",
    "activity-consumer:watch": "NODE_ENV=development TZ=UTC nodemon --inspect=127.0.0.1:9228 src/jobs/activity-consumer.js",
    "email-activity-consumer:run": "NODE_ENV=development TZ=UTC node src/jobs/email-activity-consumer.js",
    "prepare:e2e": "NODE_ENV=e2e node src/scripts/add-roles.js && NODE_ENV=e2e node src/scripts/add-team-roles.js && NODE_ENV=e2e node src/scripts/add-plans.js && NODE_ENV=e2e node src/scripts/add-reactions.js ",
    "push-activity-consumer:run": "NODE_ENV=development TZ=UTC node src/jobs/push-activity-consumer.js"
  },
  "repository": {
    "type": "git",
    "url": "git clone git@bitbucket.org:helloivy/complex-be.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/helloivy/complex-be#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@faker-js/faker": "^8.3.1",
    "are-you-es5": "^2.1.1",
    "babel-plugin-module-extension": "^0.1.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.309.0",
    "@elastic/elasticsearch": "7.10",
    "@hapi/joi": "^17.1.1",
    "@hocuspocus/extension-database": "^2.0.6",
    "@hocuspocus/extension-logger": "^2.0.6",
    "@hocuspocus/extension-throttle": "^2.0.6",
    "@hocuspocus/server": "^2.0.6",
    "@hocuspocus/transformer": "^2.0.6",
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "@onesignal/node-onesignal": "^2.0.1-beta2",
    "@sendgrid/mail": "^7.2.3",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-blockquote": "^2.0.3",
    "@tiptap/extension-bold": "^2.0.3",
    "@tiptap/extension-bullet-list": "^2.0.3",
    "@tiptap/extension-code-block-lowlight": "^2.0.3",
    "@tiptap/extension-document": "^2.0.3",
    "@tiptap/extension-hard-break": "^2.0.3",
    "@tiptap/extension-highlight": "^2.0.3",
    "@tiptap/extension-horizontal-rule": "^2.0.3",
    "@tiptap/extension-italic": "^2.0.3",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/extension-table-row": "^2.0.3",
    "@tiptap/extension-text": "^2.0.3",
    "@tiptap/extension-underline": "^2.0.3",
    "@tiptap/extension-youtube": "^2.1.12",
    "@tiptap/html": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "aws-sdk": "^2.735.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "build-url": "^2.0.0",
    "cloudconvert": "^2.1.8",
    "cloudmersive-convert-api-client": "^2.5.6",
    "debounce": "^1.2.1",
    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "googleapis": "^112.0.0",
    "html-docx-js": "^0.1.0",
    "html-pdf": "^3.0.1",
    "html-pdf-chrome": "^0.6.1",
    "html-to-docx": "^1.1.29",
    "image-to-base64": "^2.1.1",
    "jsdom": "^16.5.2",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-easy-ws": "^1.3.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^6.0.0",
    "koa-router": "^9.4.0",
    "log4js": "^6.3.0",
    "loops": "^2.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "nanoid-dictionary": "^3.0.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "pg": "^8.3.2",
    "pg-native": "^3.0.0",
    "prosemirror-model": "^1.18.1",
    "prosemirror-view": "^1.31.2",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "rrule": "^2.7.2",
    "sequelize": "^6.3.4",
    "stripe": "^8.88.0",
    "umzug": "^2.3.0",
    "uuid": "^8.3.0",
    "y-prosemirror": "^1.2.1",
    "y-protocols": "^1.0.5",
    "yjs": "^13.6.1"
  }
}
